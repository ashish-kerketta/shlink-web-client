{
    "globals": {
        "document": true,
        "escape": true,
        "window": true,
        "localStorage": true,
        "navigator": true,
        "FormData": true,
        "FileReader": true,
        "WebFontConfig": true,
        "angular" : true,
        "moment": true,
        "jasmine": true,
        "describe": true,
        "xdescribe": true,
        "before": true,
